# API Keys
SERPER_API_KEY=your_serper_key_here  # https://serper.dev/ (free tier)
OPENAI_API_KEY=your_open_api_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here  # Optional: for Claude models
GOOGLE_API_KEY=your_google_key_here       # Optional: for Gemini models
OPENROUTER_API_KEY=your_open_router_key_here

# Default LLM Configuration (Optional)

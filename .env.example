# API Keys
SERPER_API_KEY=your_serper_key_here  # https://serper.dev/ (free tier)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here  # Optional: for Claude models
GOOGLE_API_KEY= your_google_api_key_here      # Optional: for Gemini models
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Default LLM Configuration (Optional)
DEFAULT_LLM_PROVIDER=your_default_llm_provider  # Options: openai, anthropic, google, ollama, openrouter
DEFAULT_LLM_MODEL=your_default_llm_model  # Optional: will use provider default if not set
LLM_TEMPERATURE=0.1
LLM_MAX_TOKENS=3000

# Ollama Configuration (if using local models)
OLLAMA_BASE_URL=http://localhost:11434  # Default Ollama server URL


GOOGLE_APPLICATION_CREDENTIALS = path_to_credentials_file  # Path to your Google Cloud credentials file
